ull,"original":{"line":284,"column":6},"generated":{"line":1,"column":3482}},{"source":"src/sass/styles.scss","name":null,"original":{"line":284,"column":6},"generated":{"line":1,"column":3503}},{"source":"src/sass/styles.scss","name":null,"original":{"line":285,"column":6},"generated":{"line":1,"column":3504}},{"source":"src/sass/styles.scss","name":null,"original":{"line":285,"column":6},"generated":{"line":1,"column":3514}},{"source":"src/sass/styles.scss","name":null,"original":{"line":286,"column":6},"generated":{"line":1,"column":3515}},{"source":"src/sass/styles.scss","name":null,"original":{"line":288,"column":6},"generated":{"line":1,"column":3534}},{"source":"src/sass/styles.scss","name":null,"original":{"line":289,"column":8},"generated":{"line":1,"column":3553}},{"source":"src/sass/styles.scss","name":null,"original":{"line":289,"column":8},"generated":{"line":1,"column":3563}},{"source":"src/sass/styles.scss","name":null,"original":{"line":290,"column":8},"generated":{"line":1,"column":3564}},{"source":"src/sass/styles.scss","name":null,"original":{"line":293,"column":6},"generated":{"line":1,"column":3575}},{"source":"src/sass/styles.scss","name":null,"original":{"line":294,"column":8},"generated":{"line":1,"column":3596}},{"source":"src/sass/styles.scss","name":null,"original":{"line":296,"column":6},"generated":{"line":1,"column":3607}},{"source":"src/sass/styles.scss","name":null,"original":{"line":297,"column":8},"generated":{"line":1,"column":3629}},{"source":"src/sass/styles.scss","name":null,"original":{"line":297,"column":8},"generated":{"line":1,"column":3642}},{"source":"src/sass/styles.scss","name":null,"original":{"line":298,"column":8},"generated":{"line":1,"column":3643}},{"source":"src/sass/styles.scss","name":null,"original":{"line":298,"column":8},"generated":{"line":1,"column":3654}},{"source":"src/sass/styles.scss","name":null,"original":{"line":299,"column":8},"generated":{"line":1,"column":3655}}],"sources":{"src/sass/styles.scss":"/* styles.css */\n\n// Variables\n$color-green: #abc270;\n$color-sand: #fec868;\n$color-clay: #fda769;\n$color-coal: #473c33;\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody,\nhtml {\n  font-family: system-ui, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto,\n    Oxygen, Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\n  overflow-x: hidden;\n}\n\na {\n  text-decoration: none;\n  color: $color-coal;\n}\n\n.btn {\n  padding: 7px 15px;\n  font-weight: 700;\n  outline: 0;\n  border: none;\n  border-radius: 5px;\n  background: $color-sand;\n  cursor: pointer;\n}\n\n.btn:hover {\n  background: #473c33;\n  color: white;\n}\n\nheader {\n  position: sticky;\n  top: 0;\n  z-index: 999;\n  background: white;\n  display: flex;\n  align-items: center;\n  flex-wrap: wrap;\n  gap: 20px;\n  padding: 20px;\n  // margin: 30px;\n  border-radius: 8px;\n  box-shadow: rgba(0, 0, 0, 0.15) 1.95px 1.95px 2.6px;\n\n  .close-icon {\n    display: none;\n    cursor: pointer;\n  }\n\n  .search-bar {\n    width: 500px;\n\n    #search-form {\n      display: flex;\n      gap: 5px;\n      input {\n        width: 100%;\n        border-radius: 12px;\n        border: 2px solid $color-sand;\n        padding: 10px 0px 10px 10px;\n      }\n      input:focus {\n        border-color: black;\n        outline: 0;\n      }\n    }\n  }\n\n  select {\n    outline: 0;\n    border: none;\n    font-size: 1rem;\n  }\n}\n\nmain {\n  // HERO\n  #hero {\n    width: 100%;\n    height: 60vh;\n    padding: 50px;\n    margin-bottom: 150px;\n    display: flex;\n\n    .hero-left {\n      width: 100%;\n      .hero-text {\n        align-self: flex-start;\n        padding: 80px 80px 20px 80px;\n        font-size: 2rem;\n        color: black;\n\n        .btn-hero {\n          margin-top: 40px;\n          font-size: 1.2rem;\n          padding: 15px 50px;\n          transition: transform 1s;\n          min-width: 200px;\n        }\n\n        .btn-hero:hover {\n          transform: scale(1.2);\n        }\n      }\n    }\n\n    .hero-right {\n      width: 100%;\n      .hero-img {\n        img {\n          width: 600px;\n        }\n      }\n    }\n  }\n\n  .btn {\n    padding: 5px 15px;\n  }\n  // NEW BOOK FORM SECTION\n\n  #new-book-form {\n    background: $color-coal;\n    padding: 20px;\n    border-radius: 12px;\n    display: flex;\n    justify-content: center;\n    align-content: center;\n    flex-wrap: wrap;\n    gap: 10px;\n    margin: 30px;\n    height: auto;\n\n    div {\n      display: flex;\n      flex-direction: column;\n    }\n\n    input {\n      height: 40px;\n    }\n\n    input:focus {\n      border-color: $color-sand;\n      outline: 0;\n    }\n\n    label {\n      color: white;\n    }\n    .btn:hover {\n      background: $color-green;\n    }\n  }\n\n  //   BOOKS SECTION\n  #books {\n    min-height: 500px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 20px;\n\n    h2 {\n      margin: 0px 0px 30px;\n    }\n\n    .books-container {\n      display: flex;\n      flex-wrap: wrap;\n      justify-content: center;\n      gap: 20px;\n\n      .single-book__img {\n        img {\n          height: 300px;\n          object-fit: cover;\n          object-position: center top;\n          width: 100%;\n        }\n      }\n      .single-book {\n        border-radius: 10px;\n        background: white;\n        box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);\n        width: 300px;\n        height: auto;\n        overflow: hidden;\n        cursor: pointer;\n\n        .book-info {\n          margin: 10px;\n\n          div {\n            display: flex;\n            align-items: center;\n            gap: 5px;\n          }\n\n          p,\n          h3 {\n            margin: 4px 0px;\n            max-width: 200px;\n            max-height: 24px;\n            text-overflow: ellipsis;\n            overflow: hidden;\n            white-space: nowrap;\n          }\n\n          .price {\n            font-size: 1.3rem;\n            font-weight: 700;\n          }\n        }\n\n        .book-controls {\n          display: flex;\n          gap: 10px;\n          margin: 10px;\n        }\n      }\n    }\n  }\n}\nfooter {\n  background: $color-coal;\n  color: white;\n  width: 100%;\n  margin-top: 60px;\n\n  div {\n    padding: 30px;\n    display: flex;\n    align-items: center;\n  }\n}\n\n@media screen and (max-width: 1376px) {\n  header {\n    .close-icon {\n      display: inline-block;\n      font-size: 1.8rem;\n      position: absolute;\n      top: 15px;\n      left: 90%;\n      color: rgba(0, 0, 0, 0.8);\n    }\n\n    .burger {\n      display: inline-block;\n    }\n    transition: top 1s;\n    position: relative;\n    top: -300px;\n  }\n  #hero {\n    flex-direction: column;\n    img {\n      display: none;\n    }\n\n    svg {\n      margin: 300px 200px;\n      width: 100%;\n    }\n  }\n\n  #new-book {\n    display: flex;\n    justify-content: center;\n\n    form {\n      flex-direction: column;\n      width: 100%;\n      align-items: center;\n\n      div {\n        width: 100%;\n        margin: 5px;\n      }\n\n      input {\n        width: 100%;\n      }\n      button {\n        margin: 20px 0px;\n        height: 60px;\n        width: 200px;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"1210c7ed0a0773b91debf7fa5228d4d7","cacheData":{"env":{}}}