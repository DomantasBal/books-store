...storedBooks, book];\n\n  //   SETS UPDATED BOOKS ARRAY IN LOCALSTORAGE\n  localStorage.setItem(\"books\", JSON.stringify(updatedBooks));\n  form.reset();\n}\n\n// FORM SUBMIT EVENT TRIGGER\nconst form = document.getElementById(\"new-book-form\");\nform.addEventListener(\"submit\", addNewBook);\n\nfunction bookTemplate(book) {\n  return `\n    <article id=book${book.id} class=\"single-book\">\n    <img src=\"./src/img/book-test.jpg\" alt=\"\" />\n    <div class=\"book-info\">\n      <h3 class=\"single-book__name\">${book.name}</h3>\n      <p class=\"single-book__author\">${book.author}</p>\n      <p class=\"single-book_category\">${book.category}</p>\n      <p class=\"single-book__years\">${book.year}</p>\n      <p class=\"single-book__price\">${book.price}€</p>\n    </div>\n    <div class=\"book-controls\">\n      <button class=\"btn edit\" onclick=\"editBook()\">Edit</button>\n      <button class=\"btn delete\">Delete</button>\n    </div>\n  </article>\n      `;\n}\n\nfunction editBook() {\n  console.log(\"ldksflfkj\");\n}\n\nfunction showBooks() {\n  let books = JSON.parse(localStorage.getItem(\"books\"));\n  const booksContainer = document.querySelector(\".books-container\");\n  books.forEach((book) => {\n    booksContainer.innerHTML += bookTemplate(book);\n  });\n}\n\nwindow.onload = showBooks;\n"},"lineCount":null}},"error":null,"hash":"d8e8107fcdb52fe15ff1b5b3f3d7827a","cacheData":{"env":{}}}